                     <script>
     document.addEventListener('DOMContentLoaded', function () {
         var lazyLoadElementsUp = document.querySelectorAll('.fade-in');
         var lazyLoadElementsDown = document.querySelectorAll('.fade-out');

         var lazyLoadElementsLf = document.querySelectorAll('.fade-left');
         var lazyLoadElementsRg = document.querySelectorAll('.fade-right');

         var lazyLoadThreshold = 200; // Số pixel từ trên cùng của trang để kích hoạt hiệu ứng

         function checkLazyLoad() {
             lazyLoadElementsUp.forEach(function (element) {
                 var rect = element.getBoundingClientRect();
                 if (rect.top <= window.innerHeight + lazyLoadThreshold) {
                     element.classList.add('visible');
                 }
             });
             lazyLoadElementsDown.forEach(function (element) {
                 var rect = element.getBoundingClientRect();
                 if (rect.top <= window.innerHeight + lazyLoadThreshold) {
                     element.classList.add('visible');
                 }
             });
             lazyLoadElementsLf.forEach(function (element) {
                 var rect = element.getBoundingClientRect();
                 if (rect.top <= window.innerHeight + lazyLoadThreshold) {
                     element.classList.add('visible');
                 }
             });
             lazyLoadElementsRg.forEach(function (element) {
                 var rect = element.getBoundingClientRect();
                 if (rect.top <= window.innerHeight + lazyLoadThreshold) {
                     element.classList.add('visible');
                 }
             });
         }

         window.addEventListener('scroll', checkLazyLoad);
         window.addEventListener('resize', checkLazyLoad);

         // Kiểm tra lần đầu khi trang tải
         checkLazyLoad();
     });
                     </script>
