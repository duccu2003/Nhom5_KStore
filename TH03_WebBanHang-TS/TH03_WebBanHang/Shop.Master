 <script>
     // Hàm để kiểm tra vị trí cuộn và hiển thị/ẩn nút "Lên"
     function checkScrollPosition() {
         var moveTop = document.getElementById('moveTop');
         if (window.scrollY >= 400) { // Kiểm tra nếu cuộn quá 100vh
             moveTop.style.display = 'block'; // Hiển thị nút "Lên"
         } else {
             moveTop.style.display = 'none'; // Ẩn nút "Lên"
         }
     }

     // Thêm sự kiện listener cho sự kiện `scroll`
     window.addEventListener('scroll', checkScrollPosition);

     // Gọi hàm checkScrollPosition đầu tiên để đảm bảo nút "Lên" được hiển thị/ẩn đúng từ đầu
     checkScrollPosition();

