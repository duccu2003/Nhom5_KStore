         .fade-in {
        opacity: 0;
        transform: translateY(100px);
        transition: opacity 0.8s, transform 0.8s;
        }

        .fade-out {
            opacity: 0;
            transform: translateY(-100px);
            transition: opacity 0.8s, transform 0.8s;
        }

        .fade-left {
            opacity: 0;
            transform: translateX(-150px);
            transition: opacity 1.5s, transform 1.5s;
        }

        .fade-right {
            opacity: 0;
            transform: translateX(150px);
            transition: opacity 1.5s, transform 1.5s;
        }


        .fade-in.visible {
            opacity: 1;
            transform: translateY(0);
        }

        .fade-out.visible {
            opacity: 1;
            transform: translateY(0);
        }

        .fade-left.visible {
            opacity: 1;
            transform: translateX(0);
        }

        .fade-right.visible {
            opacity: 1;
            transform: translateX(0);
        }


